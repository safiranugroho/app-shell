{
  "name": "app-shell",
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "static/*.svg",
      "use": "@now/static"
    },
    {
      "src": "static/*.ico",
      "use": "@now/static"
    }
  ],
  "alias": [
    "app-shell"
  ]
}